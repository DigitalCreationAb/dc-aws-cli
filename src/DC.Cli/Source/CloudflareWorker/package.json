{
  "private": true,
  "name": "[[WORKER_NAME]]",
  "version": "0.1.0",
  "description": "A Cloudflare Worker [[WORKER_NAME]]",
  "scripts": {
    "build": "yarn install && webpack --mode=development index.js -o dist/main.js",
    "start": "yarn install && nodemon --watch dist/main.js --exec \"cloudflare-worker-local dist/main.js 172.17.0.1:$DESTINATION_PORT 3000 wrangler.toml local\"",
    "watch": "yarn install && webpack --mode=development index.js -o dist/main.js --watch"
  },
  "devDependencies": {
    "cloudflare-worker-local": "^1.12.1",
    "nodemon": "^2.0.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "aws4fetch": "^1.0.5"
  }
}
